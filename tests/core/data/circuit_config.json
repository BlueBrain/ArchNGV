{
  "manifest": {
    "$BASE_DIR": ".",
    "$COMPONENT_DIR": "$BASE_DIR",
    "$NETWORK_DIR": "./"
  },
  "components": {
    "morphologies_dir": "$COMPONENT_DIR/morphologies"
  },
  "networks": {
    "nodes": [
      {
        "nodes_file": "$NETWORK_DIR/glia.h5",
        "node_types_file": null
      },
      {
        "nodes_file": "$NETWORK_DIR/nodes.h5",
        "node_types_file": null
      }
    ],
    "edges": [
      {
        "edges_file": "$NETWORK_DIR/glialglial.h5",
        "edge_types_file": null
      },
      {
        "edges_file": "$NETWORK_DIR/neuroglial.h5",
        "edge_types_file": null
      },
      {
        "edges_file": "$NETWORK_DIR/edges.h5",
        "edge_types_file": null
      }
    ],
    "vasculature": {
      "vasculature_file": "$NETWORK_DIR/vasculature.h5",
      "vasculature_mesh_file": "$NETWORK_DIR/vasculature_mesh.obj"
    },
    "astrocytes": [
      {
        "population": "astrocytes",
        "microdomains_file": "$NETWORK_DIR/microdomains.h5",
        "microdomains_overlapping_file": "$NETWORK_DIR/overlapping_microdomains.h5",
        "endfeet_file": "$NETWORK_DIR/endfeet_areas.h5",
        "endfeet_data_file": "$NETWORK_DIR/gliovascular_data.h5",
        "morphologies_dir": "$NETWORK_DIR/morphologies"
      }
    ],
    "gliovascular": "$NETWORK_DIR/gliovascular_connectivity.h5",
    "atlases": {
      "my_atlas": "$NETWORK_DIR/my_atlas.nrrd"
    }
  }
}
